<% if cameo.nil? %>
  <img alt="" src="/assets/video_img.png">
<% else %>
  <div id="kaltura_player_<%= cameo.show_id %>" style="width:565px;height:353px;"></div> 
  <% content_for :js_with_script_tags do %>
    <script src="http://cdnapi.kaltura.com/p/<%= configatron.kaltura_partner_id %>/sp/<%= configatron.kaltura_partner_id %>00/embedIframeJs/uiconf_id/14018252/partner_id/<%= configatron.kaltura_partner_id %>"></script>
    <script>
      var PARTNER_ID = "<%= configatron.kaltura_partner_id %>"
      var ENTRY_ID = "<%= cameo.kaltura_entry_id %>"
      var TARGET_DIV_ID = "kaltura_player_<%= cameo.show_id %>"
      loadKalturaPlayer(PARTNER_ID, ENTRY_ID, TARGET_DIV_ID);
    </script>
  <% end %>
<% end %>
