<div class="myvision_container">
  <div class="row-fluid">
    <div class="span12 frinds_container_padding">
      <div class="myvision_title "> A little more about yourself
      </div>
      <div style="padding-left: 100px;margin-top: 15px;font-weight: bold;"> 
        <ul class=""  >
          <li><%= link_to "Click here to set your profile picture", profile_user_path(current_user.id)%></li>
          <li><%= link_to "Add Description about your self ", profile_user_path(current_user.id)%></li>
          <li><%= link_to "Click here to add your profile video", profile_user_path(current_user.id)%></li>
        </ul>
      </div>
    </div>

    <div class="row-fluid"> 
      <div class="span12 frinds_container_padding">
        <div class="myvision_title"> Next steps
        </div>
        <div style="padding-left: 100px;margin-top: 15px;font-weight: bold;"> 
          <ul class=""> 

            <% comment do %>
              <% if @notifications.count>0 %>
                <li class="leaf">  <div class="font_weight_bold">You have <%= @notifications.count %>  new <%= link_to "friend Requests ", notification_user_path(current_user.id) %></div></li>
              <% end %>
            <% end %>

            <% if @show_notifications %>
              <% @show_notifications.each do |each_notification| %>
                <% show = Show.find(each_notification.show_id) if each_notification.show_id.present?%>
                <% user = User.find(show.user_id) if show.present?%>
                <% if user%>
                 <li class="leaf">  <div class="font_weight_bold"> <%= user.first_name %> has invited you to contribute to her video <%= link_to "#{show.title}", shows_path(:id => show.id,  :to_contribute=>"yes") %><br/>view the video and contribute!</div></li>
                <% end %>
              <% end %>
            <% end %>
              <!-- <li class="leaf">your watching <b> Title goes here. </b> Go back to the video and <br> contribute! </li> -->
          </ul>
        </div>
      </div>
    </div>
    <div class="row-fluid"> 
      <div class="span12"> 
        <div class="register_hr_bg"> 
          <div class="text_align_center login_and_account"> 
            <%= link_to raw("<i class=\"sprite-camera-white \"></i>Start a new show"), new_show_path, :class=>"btn btn-warning " %>
         </div>
       </div>
     </div>
   </div>
 </div>
</div>
