<% if current_user.present? && @cameo.show.user_id!=current_user.id %>
  <%= form_tag check_password_web_cameos_path do %>
    <div class="login_container">
      <div class="">
        <h3 id="myModalLabel">This Show is Password Protected</h3>
      </div>
      <div class="">
        <div class="control-group">
          <label class="control-label" for="inputEmail">Password</label>
          <div class="controls">
            <%= password_field_tag :password %>
            <%= hidden_field_tag :show_id, params[:show_id] %>
          </div>
        </div>
      </div>
      <div class="">
        <%= link_to "Close", root_url, :class => "btn btn-info" %>
        <%= submit_tag "Submit", :class => "btn btn-warning" %>
      </div>
    </div>
  <% end %>
<% end %>