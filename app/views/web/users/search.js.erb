<% if @users.any? %>
  $("#div_show_friends").html("<%= j(render :partial=>"web/friends/rvidi_users",
                                            :locals => {:users=> @users,
                                                        :search_val => params[:search_value]} ) %>");
<% else %>
  $("#div_show_friends").html("<%= j(render :partial=>"web/friends/invite_user") %>")
<% end %>
