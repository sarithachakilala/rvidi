<h1>Listing cameos</h1>

<table>
  <tr>
    <th>Video</th>
    <th>User</th>
    <th>Director</th>
    <th>Status</th>
    <th>Show</th>
    <th>Show order</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cameos.each do |cameo| %>
  <tr>
    <td><%= cameo.video_id %></td>
    <td><%= cameo.user_id %></td>
    <td><%= cameo.director_id %></td>
    <td><%= cameo.status %></td>
    <td><%= cameo.show_id %></td>
    <td><%= cameo.show_order %></td>
    <td><%= link_to 'Show', cameo %></td>
    <td><%= link_to 'Edit', edit_cameo_path(cameo) %></td>
    <td><%= link_to 'Destroy', cameo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Cameo', new_cameo_path %>
